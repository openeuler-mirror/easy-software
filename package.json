    "markdown-it": "12.3.2",
    "pinia": "2.1.6",
    "vue": "3.3.4",
    "vue-dompurify-html": "3.1.2",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.4",
    "xss": "1.0.14",
    "github-markdown-css": "5.1.0"
