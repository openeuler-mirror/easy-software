    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "1.62.0",
    "typescript": "4.9.5",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-icons": "^0.14.15",
    "unplugin-vue-components": "^0.26.0",
    "vite": "4.5.3",
    "vue-tsc": "^1.8.25"
